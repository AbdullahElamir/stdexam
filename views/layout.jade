            a(href='#')
              span.fa.fa-pencil
              span.xn-text Forms
            ul
              li
                a(href='form-layouts-two-column.html')
                  span.fa.fa-tasks
                  |  Form Layouts
                .informer.informer-danger New
                ul
                  li
                    a(href='form-layouts-one-column.html')
                      span.fa.fa-align-justify
                      |  One Column
