                a(href='form-elements.html')
                  span.fa.fa-file-text-o
                  |  Elements
              li
                a(href='form-validation.html')
                  span.fa.fa-list-alt
                  |  Validation
              li
                a(href='form-wizards.html')
                  span.fa.fa-arrow-right
                  |  Wizards
              li
                a(href='form-editors.html')
                  span.fa.fa-text-width
                  |  WYSIWYG Editors
              li
