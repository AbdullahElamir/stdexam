extends layout
block content
  .page-inner
    .page-title#page-title
      h3 Dashboard
      .page-breadcrumb
        ol.breadcrumb
          li
            a(href="index.html") Home
          li.active Dashboard
    #main-wrapper
      .row
        .col-lg-3.col-md-6
          .panel.info-box.panel-white
            .panel-body
              .info-box-stats
                p.counter 107,200
                span.info-box-title User activity this month
              .info-box-icon
                i.icon-users
              .info-box-progress
                .progress.progress-xs.progress-squared.bs-n
                  .progress-bar.progress-bar-success(role="progressbar", aria-valuenow="40", aria-valuemin="0", aria-valuemax="100", style="width: 40%")
        .col-lg-3.col-md-6
          .panel.info-box.panel-white
            .panel-body
              .info-box-stats
                p.counter 340,230
                span.info-box-title Page views
              .info-box-icon
                i.icon-eye
              .info-box-progress
                .progress.progress-xs.progress-squared.bs-n
                  .progress-bar.progress-bar-info(role="progressbar", aria-valuenow="80", aria-valuemin="0", aria-valuemax="100", style="width: 80%")
        .col-lg-3.col-md-6
          .panel.info-box.panel-white
            .panel-body
              .info-box-stats
                p
                  | $
                  span.counter 653,000
                span.info-box-title Monthly revenue goal
              .info-box-icon
                i.icon-basket
              .info-box-progress
                .progress.progress-xs.progress-squared.bs-n
                  .progress-bar.progress-bar-warning(role="progressbar", aria-valuenow="60", aria-valuemin="0", aria-valuemax="100", style="width: 60%")
        .col-lg-3.col-md-6
          .panel.info-box.panel-white
            .panel-body
              .info-box-stats
                p.counter 47,500
                span.info-box-title New emails recieved
              .info-box-icon
                i.icon-envelope
              .info-box-progress
                .progress.progress-xs.progress-squared.bs-n
                  .progress-bar.progress-bar-danger(role="progressbar", aria-valuenow="50", aria-valuemin="0", aria-valuemax="100", style="width: 50%")
      // Row
      .row
        .col-lg-9.col-md-12
          .panel.panel-white
            .row
              .col-sm-8
                .visitors-chart
                  .panel-heading
                    h4.panel-title Visitors
                  .panel-body
                    #flotchart1
              .col-sm-4
                .stats-info
                  .panel-heading
                    h4.panel-title Browser Stats
                  .panel-body
                    ul.list-unstyled
                      li
                        | Google Chrome
                        .text-success.pull-right
                          | 32%
                          i.fa.fa-level-up
                      li
                        | Firefox
                        .text-success.pull-right
                          | 25%
                          i.fa.fa-level-up
                      li
                        | Internet Explorer
                        .text-success.pull-right
                          | 16%
                          i.fa.fa-level-up
                      li
                        | Safari
                        .text-danger.pull-right
                          | 13%
                          i.fa.fa-level-down
                      li
                        | Opera
                        .text-danger.pull-right
                          | 7%
                          i.fa.fa-level-down
                      li
                        | Mobile &#38; tablet
                        .text-success.pull-right
                          | 4%
                          i.fa.fa-level-up
                      li
                        | Others
                        .text-success.pull-right
                          | 3%
                          i.fa.fa-level-up
        .col-lg-3.col-md-6
          .panel.panel-white(style="height: 100%;")
            .panel-heading
              h4.panel-title Server Load
              .panel-control
                a.panel-collapse(href="javascript:void(0);", data-toggle="tooltip", data-placement="top", title="Expand/Collapse")
                  i.icon-arrow-down
                a.panel-reload(href="javascript:void(0);", data-toggle="tooltip", data-placement="top", title="Reload")
                  i.icon-reload
            .panel-body
              .server-load
                .server-stat
                  span Total Usage
                  p 67GB
                .server-stat
                  span Total Space
                  p 320GB
                .server-stat
                  span CPU
                  p 57%
              #flotchart2
        .col-lg-5.col-md-6
          .panel.panel-white
            .panel-heading
              h4.panel-title Weather
              .panel-control
                a.panel-reload(href="javascript:void(0);", data-toggle="tooltip", data-placement="top", title="Reload")
                  i.icon-reload
            .panel-body
              .weather-widget
                .row
                  .col-md-12
                    .weather-top
                      .weather-current.pull-left
                        i.wi.wi-day-cloudy.weather-icon
                        p
                          span
                            | 83
                            sup &#176;F
                      h2.weather-day.pull-right
                        | Miami, FL
                        br
                        small
                          b 13th April, 2015
                  .col-md-6
                    ul.list-unstyled.weather-info
                      li
                        | Wind
                        span.pull-right
                          b ESE 16 mph
                      li
                        | Humidity
                        span.pull-right
                          b 64%
                      li
                        | Pressure
                        span.pull-right
                          b 30.15 in
                      li
                        | UV Index
                        span.pull-right
                          b 6
                  .col-md-6
                    ul.list-unstyled.weather-info
                      li
                        | Cloud Cover
                        span.pull-right
                          b 60%
                      li
                        | Ceiling
                        span.pull-right
                          b 17800 ft
                      li
                        | Dew Point
                        span.pull-right
                          b 70&#176; F
                      li
                        | Visibility
                        span.pull-right
                          b 10 mi
                  .col-md-12
                    ul.list-unstyled.weather-days.row
                      li.col-xs-4.col-sm-2
                        span 12:00
                        i.wi.wi-day-cloudy
                        span
                          | 82
                          sup &#176;F
                      li.col-xs-4.col-sm-2
                        span 13:00
                        i.wi.wi-day-cloudy
                        span
                          | 82
                          sup &#176;F
                      li.col-xs-4.col-sm-2
                        span 14:00
                        i.wi.wi-day-cloudy
                        span
                          | 82
                          sup &#176;F
                      li.col-xs-4.col-sm-2
                        span 15:00
                        i.wi.wi-day-cloudy
                        span
                          | 83
                          sup &#176;F
                      li.col-xs-4.col-sm-2
                        span 16:00
                        i.wi.wi-day-cloudy
                        span
                          | 82
                          sup &#176;F
                      li.col-xs-4.col-sm-2
                        span 17:00
                        i.wi.wi-day-sunny-overcast
                        span
                          | 82
                          sup &#176;F
        .col-lg-4.col-md-6
          .panel.panel-white
            .panel-heading
